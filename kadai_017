class Human:
    def __init__(self, name, age):
        self.name = name
        self.age = age
    
    def check_adult(self):
        if self.age >= 20:
            print(f"{self.name}は大人です")
        else:
            print(f"{self.name}は大人ではありません")

human = []
human.append(Human("太郎", 32))
human.append(Human("花子", 24))
human.append(Human("次郎", 15))

for human in human:
    human.check_adult()